extends layouts/main
include mixins/postingList
block content
    .postings
                each item in posts
                        +postingList({
                            id : item._id,
                            title: item.title,
                            description: item.description,
                            creator : item.creator,
                            creatorName : item.creatorName,
                            like : item.like,
                            user : item.creator
                        }) 